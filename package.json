{
  "name": "gym-member-backend",
  "version": "1.0.0",
  "description": "Backend API for gym member management system",
  "main": "app.js",
  "type": "commonjs",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "db:push": "prisma db push",
